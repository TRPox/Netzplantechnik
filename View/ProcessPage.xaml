<Page x:Class="Netzplantechnik.View.ProcessPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="500"
	Title="ProcessPage">

    <StackPanel>
        <DockPanel Height="50" LastChildFill="False">
            <Button Content="Neuer Prozess" VerticalAlignment="Top" Width="100" Height="25" Margin="10,10,0,0"/>
            <Button Content="Prozess löschen" VerticalAlignment="Top" Width="100" Height="25" Margin="10,10,0,0"/>
            <Button Content="Berechnen" VerticalAlignment="Top" Width="100" Height="25" Margin="10,10,0,0"/>
        </DockPanel>
        <Grid x:Name="ProcessGrid" Height="100"/>




    </StackPanel>
</Page>
